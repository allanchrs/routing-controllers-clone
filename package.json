{
  "name": "@allanvieira/google-drive-api",
  "author": {
    "name": "Allan Vieira",
    "email": "allancnfx.vieira@gmail.com"
  },
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "dev": "tsnd --inspect --transpile-only --ignore-watch -r tsconfig-paths/register src/server/index.ts",
    "start": "node server/index.js",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test": "jest --coverage=false",
    "test:watch": "jest --watch --coverage=false",
    "test:cov": "jest --coverage=true",
    "typeorm": "typeorm-ts-node-commonjs -d ./dist/src/database/data-source.js",
    "typeorm:dev": "typeorm-ts-node-commonjs -d ./src/database/data-source.ts",
    "migration:gen": "read name && yarn typeorm:dev migration:generate ./src/database/migrations/$name",
    "migration:create": "read name && npx typeorm-ts-node-commonjs migration:create ./src/database/migrations/$name",
    "migration:run:dev": "yarn typeorm:dev migration:run",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm:dev migration:revert"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.515.0",
    "dotenv": "^16.4.4",
    "pg": "^8.11.3",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "pretty-bytes": "^6.1.1",
    "reflect-metadata": "^0.2.1",
    "socket.io": "^4.7.4",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
